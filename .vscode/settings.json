{
    "java.compile.nullAnalysis.mode": "automatic",
    "liveServer.settings.port": 5501, // Or your port
    "liveServer.settings.ignoreFiles": [
        ".vscode/**",
        "**/*.scss",
        "**/*.sass",
        "**/node_modules/**",
        "**/bower_components/**",
        "**/.git/**",
        "jewellery_backend/uploads/**" // This tells Live Server to ignore the uploads folder
    ]
}